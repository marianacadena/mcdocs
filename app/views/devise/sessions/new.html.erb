<nav class="navbar navbar-expand-sm bg-dark navbar-dark sticky-top" >
  <a class="navbar-brand" href="#">
    <%= image_tag("MCDocs_Logo.png", :alt => "logo", :id => "img-logo") %>
  </a>
  <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon" ></span>
  </button>
  <div class="collapse navbar-collapse" id="navbarResponsive">
    <ul class="navbar-nav" >
      <li class="nav-item">
        <a class="link-nav-dark" href="#sobre_nosotros">Sobre nosotros</a>
      </li>
      <li class="nav-item">
        <a class="link-nav-dark" href="#contactanos">Contactanos</a>
      </li>
    </ul>
  </div>
</nav>
<section class="login-block">
  <div class="wrapper fadeInDown">
    <div id="formContent">
      <div class="fadeIn first">
        <img src="MCDocs_Logo.png" id="icon" alt="User Icon" />
        <h2 class="text-center">Inicio de sesión</h2>
      </div>
      <%= form_for(resource, as: resource_name, url: session_path(resource_name), html: {class: "login-form"}) do |f|%>
        <div class="form-group">
          <%#= f.label :email, value: "Email", class: "text-uppercase label-as-block"%>
          <%= f.email_field :email, autofocus: true, autocomplete: "email", class: "fadeIn second", placeholder: "Email" %>
        </div>
        <div class="form-group">
          <%#= f.label :password, value: "Clave",  class: "text-uppercase label-as-block" %>
          <%= f.password_field :password, autocomplete: "current-password", class: " fadeIn third", placeholder: "Contraseña" %>
        </div>
        <% if devise_mapping.rememberable? -%>
          <div class="form-check">
            <%= f.check_box :remember_me, class: "form-check-input"%>
            <%= f.label :remember_me, :value => "Mantener sesión iniciada", class: "form-check-label" %>
        <% end -%>
        <%= f.submit "Entrar", class: "fadeIn fourth" %>
        </div>
      <% end %>
      <div id="formFooter">
        <% if devise_mapping.registerable? && controller_name != 'registrations' %>
          <%= link_to "Registrate", new_registration_path(resource_name), class: "underlineHover " %>
        <% end %>
      </div>
    </div>
  </div>
</section>

